#Â Initialize bashrc
source $SSHHOME/.sshrc.d/.bashrc;

# Initialize vim
export VIMINIT="let \$MYVIMRC='$SSHHOME/.sshrc.d/.vimrc' | source \$MYVIMRC"

# Initialize lesskey
if hash lesskey 2>/dev/null
then
    lesskey $SSHHOME/.sshrc.d/.lesskey
fi

# Define tmux alias
alias tmux="tmux -f $SSHHOME/.sshrc.d/.tmux.conf"
