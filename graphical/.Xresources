URxvt.termName:              rxvt-256color
URxvt.internalBorder:        6
URxvt.font:                  xft:Inconsolata for Powerline:size=13
URxvt.geometry:              80x20
URxvt.depth:                 32
URxvt.background:	         [95]#263238
URxvt.scrollBar:             false
URxvt.mouseWheelScrollPage:  false
URxvt.cursorBlink:           true
URxvt.foreground: 		     #eceff1
URxvt.cursorColor:		     #eceff1
URxvt.saveLines:             50000

URxvt.fading:                30
URxvt.intensityStyles:       false
URxvt.iso14755:              false
URxvt.iso14755_52:           false
URxvt.loginShell:            true
URxvt.matcher.button:        1
URxvt.matcher.rend.0:        Uline Bold fg5
URxvt.perl-ext-common:       keyboard-select,matcher,resize-font
URxvt.scrollTtyKeypress:     true
URxvt.scrollTtyOutput:       false
URxvt.scrollWithBuffer:      false
URxvt.skipScroll:            true
URxvt.urgentOnBell:          true
URxvt.url-launcher:          /usr/bin/google-chrome
URxvt.visualBell:            false
URxvt.keysym.M-g: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true

rofi.color-enabled: true
rofi.color-window: #263238, #03a9f4, #81d4fa
rofi.color-normal: #263238, #bcbcbc, #263238, #03a9f4, #263238
rofi.color-active: #263238, #bcbcbc, #263238, #03a9f4, #263238
rofi.color-urgent: #263238, #bcbcbc, #263238, #03a9f4, #263238

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: inconsolata 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
